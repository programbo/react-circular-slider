{
  "name": "react-circular-slider",
  "version": "1.1.0",
  "description": "Flexible slider widget for Preact",
  "main": "dist/index.js",
  "source": "src/index.tsx",
  "typings": "dist/index.d.ts",
  "directories": {
    "test": "test"
  },
  "dependencies": {
    "d3-scale": "^1.0.7",
    "d3-shape": "^1.2.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.6",
    "@types/d3-scale": "^1.0.14",
    "@types/d3-shape": "^1.2.4",
    "@types/node": "^8.10.36",
    "@types/react": "^16.4.18",
    "microbundle": "^0.7.0-beta.0",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "typescript": "^3.1.3",
    "typescript-cssproperties": "^1.0.2"
  },
  "peerDependencies": {
    "react": "^16.5.2"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "tsc",
    "dev": "tsc && tsc -w",
    "bundle:helpers": "microbundle -i src/helpers/geometry.ts  -o dist/circular-slider-helpers.js --target browser --name circularSliderHelpers --globalsd3-scale=d3 --no-compress",
    "bundle": "microbundle -i src/index.tsx -o dist/circular-slider.js --target browser --jsx react --name circularSlider --globals react=React,react-dom=ReactDOM,d3-shape=d3,d3-scale=d3 --no-compress"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/programbo/preact-circular-slider.git"
  },
  "author": "John Lombardo <john@programbo.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/programbo/preact-circular-slider/issues"
  },
  "homepage": "https://github.com/programbo/preact-circular-slider#readme"
}
